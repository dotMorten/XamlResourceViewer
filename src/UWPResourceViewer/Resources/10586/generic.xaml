    <!-- Default style for Windows.UI.Xaml.Controls.Maps.MapControl -->
    <Style xmlns:maps="using:Windows.UI.Xaml.Controls.Maps" TargetType="maps:MapControl">
      <Setter Property="Background" Value="Black" />
      <Setter Property="Template">
        <Setter.Value>
          <ControlTemplate TargetType="maps:MapControl">
            <Grid Background="{TemplateBinding Background}">
              <Grid x:Name="RootGrid" HorizontalAlignment="Left" VerticalAlignment="Top">
                <Grid x:Name="MapGrid" />
                <Grid x:Name="LayerGrid" />
              </Grid>
              <Grid x:Name="UiGrid" />
            </Grid>
          </ControlTemplate>
        </Setter.Value>
      </Setter>
    </Style>


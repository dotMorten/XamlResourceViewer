                                <TickBar x:Name="HorizontalInlineTickBar"
                                        Visibility="Collapsed"
                                        Fill="{ThemeResource SystemControlBackgroundAltHighBrush}"
                                        Height="{ThemeResource SliderTrackThemeHeight}"
